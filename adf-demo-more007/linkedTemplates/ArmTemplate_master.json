{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-demo-more007"
		},
		"ls_ablob_adfdemobhushan_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_adfdemobhushan'"
		},
		"ls_adls_adfdemodl007_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_adfdemodl007'"
		},
		"ls_sql_adf_demo_db_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_adf_demo_db'"
		},
		"ls_adls_adfdemodl007_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfdemodl007.dfs.core.windows.net/"
		},
		"ls_sql_adf_demo_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "adf-demo-sql-server007.database.windows.net"
		},
		"ls_sql_adf_demo_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "adf-demo-sql-db"
		},
		"ls_sql_adf_demo_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "serveradmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-demo-more007_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_adfdemobhushan_connectionString": {
						"value": "[parameters('ls_ablob_adfdemobhushan_connectionString')]"
					},
					"ls_adls_adfdemodl007_accountKey": {
						"value": "[parameters('ls_adls_adfdemodl007_accountKey')]"
					},
					"ls_sql_adf_demo_db_password": {
						"value": "[parameters('ls_sql_adf_demo_db_password')]"
					},
					"ls_adls_adfdemodl007_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_adfdemodl007_properties_typeProperties_url')]"
					},
					"ls_sql_adf_demo_db_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_adf_demo_db_properties_typeProperties_server')]"
					},
					"ls_sql_adf_demo_db_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_adf_demo_db_properties_typeProperties_database')]"
					},
					"ls_sql_adf_demo_db_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_adf_demo_db_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}